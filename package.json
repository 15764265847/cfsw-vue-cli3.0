{
  "name": "cfsw-ssr",
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "dev:static": "vite --mode development-csr",
    "build:static": "cross-env NODE_ENV=production vite build --mode production-csr --base /cfsw/",
    "build": "vuedx-typecheck . && yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "serve:static": "cross-env NODE_ENV=csr node server-static",
    "deploy": "tcb framework deploy",
    "deploy:client": "tcb framework deploy client",
    "deploy:server": "tcb framework deploy server",
    "debug": "node --inspect-brk server",
    "create:ssr": "node ./scripts/ssr"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.0.5",
    "@wefly/vue-image-lazy-load": "^1.0.0",
    "@wefly/vue-rescroll": "^1.0.1",
    "@wefly/vue-store-next": "^1.0.1",
    "@wefly/vue-virtual-scroller": "^1.0.5",
    "axios": "^0.21.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "route-cache": "^0.4.4",
    "serialize-javascript": "^5.0.1",
    "serve-static": "^1.14.1",
    "vite-plugin-pwa": "^0.5.6",
    "vue": "^3.0.5",
    "vue-router": "^4.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.30",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vitejs/plugin-vue-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "postcss-px2rem": "^0.3.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0"
  }
}
