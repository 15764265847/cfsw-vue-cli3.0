    "vue-router": "^3.1.",
    "vue-server-renderer": "^2.6.10"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^2.3.1",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
